<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="@color/white"
    android:layout_width="match_parent" android:layout_height="match_parent">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_0_1"
        android:background="@color/white" />
    <RelativeLayout
        android:id="@+id/rl_top"
        android:layout_below="@id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_58"
        android:background="@color/white">
        <LinearLayout
            android:id="@+id/iv_back"
            android:layout_width="@dimen/dp_50"
            android:layout_alignParentBottom="true"
            android:layout_height="@dimen/dp_42">

            <ImageView
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_15"
                android:layout_gravity="bottom"
                android:layout_marginBottom="@dimen/dp_16"
                android:layout_marginLeft="@dimen/dp_15"
                android:src="@mipmap/back" />
        </LinearLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_22"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:text="群聊管理"
            android:layout_marginBottom="@dimen/dp_16"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_18"
            android:textStyle="bold" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_below="@id/rl_top"
        android:layout_height="match_parent">
        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/rl_change_headimg"
            android:layout_height="@dimen/dp_38">
            <TextView
                android:layout_width="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_15"
                android:textStyle="bold"
                android:text="群头像"
                android:textColor="@color/color_333333"
                android:textSize="@dimen/sp_15"
                android:layout_height="wrap_content">
            </TextView>
            <ImageView
                android:layout_width="@dimen/dp_33"
                android:layout_toLeftOf="@id/iv"
                android:src="@mipmap/headimg"
                android:id="@+id/iv_headimg"
                android:layout_marginRight="@dimen/dp_10"
                android:layout_centerVertical="true"
                android:layout_height="@dimen/dp_33">
            </ImageView>
            <ImageView
                android:layout_width="@dimen/dp_8"
                android:src="@mipmap/back_to"
                android:id="@+id/iv"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_15"
                android:layout_alignParentRight="true"
                android:layout_height="@dimen/dp_14">
            </ImageView>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:background="@color/color_f7f7f7"
            android:id="@+id/v1"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_below="@id/rl_change_headimg"
            android:layout_height="@dimen/dp_1">
        </View>

        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/rl_shenhelist"
            android:layout_below="@id/v1"
            android:layout_height="@dimen/dp_38">
            <TextView
                android:layout_width="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_15"
                android:textStyle="bold"
                android:text="审核列表"
                android:textColor="@color/color_333333"
                android:textSize="@dimen/sp_15"
                android:layout_height="wrap_content">
            </TextView>
            <ImageView
                android:layout_width="@dimen/dp_8"
                android:src="@mipmap/back_to"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_15"
                android:layout_alignParentRight="true"
                android:layout_height="@dimen/dp_14">
            </ImageView>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:background="@color/color_f7f7f7"
            android:id="@+id/v2"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_below="@id/rl_shenhelist"
            android:layout_height="@dimen/dp_1">
        </View>
        <RelativeLayout
            android:layout_width="match_parent"
            android:id="@+id/rl_userlist"
            android:layout_below="@id/v2"
            android:layout_height="@dimen/dp_38">
            <TextView
                android:layout_width="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dp_15"
                android:textStyle="bold"
                android:text="成员列表"
                android:textColor="@color/color_333333"
                android:textSize="@dimen/sp_15"
                android:layout_height="wrap_content">
            </TextView>
            <ImageView
                android:layout_width="@dimen/dp_8"
                android:src="@mipmap/back_to"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/dp_15"
                android:layout_alignParentRight="true"
                android:layout_height="@dimen/dp_14">
            </ImageView>
        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:background="@color/color_f7f7f7"
            android:id="@+id/v3"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_below="@id/rl_shenhelist"
            android:layout_height="@dimen/dp_1">
        </View>
    </RelativeLayout>
</RelativeLayout>